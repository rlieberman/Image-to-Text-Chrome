{
  "manifest_version": 2,
  "name": "Image-to-Text",
  "version": "1.0",
  "description": "This extension changes all images to text descriptions of the image.",
  "content_scripts": [
    {
      "matches": [
        "http://www.google.com/*",
        "https://www.google.com/*"      
      ],
      "css":["stylesheet.css"],
      "js": ["libraries/p5.js", "libraries/p5.dom.js", "libraries/jquery.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/rainbow.png"
  ]
}

