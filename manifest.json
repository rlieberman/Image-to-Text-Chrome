{
  "manifest_version": 2,
  "name": "Image-to-Text",
  "version": "1.0",
  "description": "This extension changes all images to text descriptions of the image.",
  "content_scripts": [
    {
      "matches": [
       "<all_urls>"
      ],
      "js": ["libraries/p5.js", "libraries/p5.dom.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/rainbow.png"
  ]
}

